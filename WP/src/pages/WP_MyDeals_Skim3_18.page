            var ascore = 0;
            var bscore = 0;
            
            /* Deadness */
            if ( al ) { ascore += 100; }
            if ( bl ) { bscore += 100; }
            
            /* Stage */
            if ( a < b ) {
                ascore += -10;
            } else if ( a > b ) {
                bscore += -10;
            }
            
            /* Name */
            if ( an < bn ) {
                ascore += -1;
            } else if ( an > bn ) {
                bscore += -1;
            }
             
