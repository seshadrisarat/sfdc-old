    if(dd<10) dd='0'+dd;
    if(mm<10) mm='0'+mm;
    
    var date_pick_val = mm+'/'+dd+'/'+yyyy;
    var dcr_reportType_local = 'Add Follow Up';                           
    var  Date_rcvd = new Date(date_pick_val);
    var date_2 = new Date(1980,0,1);
