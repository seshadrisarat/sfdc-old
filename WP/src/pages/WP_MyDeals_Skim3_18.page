              conversionNeeded = true;
              yearIndex = 2000 +  yearIndex;
              convertedYear = yearIndex;
              monthIndex = monthIndex + 1;
              dayIndex = i2;
              convertedDay = i2;
          }else{
            conversionNeeded = true;
            convertedYear = yearIndex;
            yearIndex = yearIndex;
            monthIndex = monthIndex + 1;
            dayIndex = i2;
          }
          convertedDate =  convertedMonth + "/" + convertedDay + "/" + convertedYear ;
          //alert("convertedDate=>" + convertedDate);
        }           
        if (conversionNeeded){
