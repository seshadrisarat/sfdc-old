                                <apex:inputField value="{!proxyContact1.Birthdate}"
                                    styleClass="inputStartDate"
                                    onchange="$('#dataloader').show(); checkDates(false,null,null,null);" />
                                <apex:outputLabel value="End: " for="pastCampaignEnd" />
