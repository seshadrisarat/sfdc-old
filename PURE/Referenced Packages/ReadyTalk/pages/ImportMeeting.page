                        value="" />
                    <apex:param name="campaignId" assignTo="{!campaignId}" value="" />
                </apex:actionFunction>

                <!-- checkCredentials = check credentials from lightbox popup to change credentials -->
                <apex:actionFunction name="checkCredentials"
