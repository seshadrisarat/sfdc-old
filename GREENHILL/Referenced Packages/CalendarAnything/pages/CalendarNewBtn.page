                function replaceSpecialChars(str) {
                    var i, regExp = /[^a-zA-Z0-9]/, res = '', char;
                    for (i=0; i<str.length; i++) {
                        char = str.substr(i,1);
                        if (regExp.test(char)) {
                            char = '_';
                        }
                        res += char;
                    }
                    
                    for (i=res.length-1; i>0; i--) {
                        char = res.substr(i,1);
                        if (char == '_') {
                            res = res.substr(0,i);
                        } else {
                            break;
                        }
                    }
                    return res;
                }
                
