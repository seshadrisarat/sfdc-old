function getUrlParameter(p) {
    var res = '';
    var url = location.href.substr(location.href.indexOf('?') + 1);
    var parts = url.split('&');
    for (var i = 0; i < parts.length; i++) {
        if (parts[i] != undefined) {
            var param = parts[i].split('=');
            if (param[0] == p) {
                res = param[1];
            }
        }
    }
    return res;
}
