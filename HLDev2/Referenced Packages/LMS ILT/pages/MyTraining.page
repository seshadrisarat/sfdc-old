                    <apex:outputLink rendered="{!IF(c.CItem.lmscons__Curriculum_Assignment__r.lmscons__Status__c =='Completed', true, false)}" value="/apex/{!IF(lmsConfig.Certificate_Page_Name=='','lmscons__LMS_Certificate','c__'+lmsConfig.Certificate_Page_Name)}?Id={!c.CItem.lmscons__Curriculum_Assignment__r.id}" styleClass="btn b-btn" style="text-decoration:none" target="_blank"><span class="m-award">&nbsp;</span></apex:outputLink>
                </apex:outputPanel>
                </apex:column>
                <!-- ---------------- -->
