                <!-- ------- -->
                <apex:column styleClass="m-col1 {!IF(ISNULL(c.TitleCurriculum), '', 'm-plus')}" rendered="{!IF(ISNULL(c.TitleCurriculum), false, IF(isConsumerUser, true, false))}">
