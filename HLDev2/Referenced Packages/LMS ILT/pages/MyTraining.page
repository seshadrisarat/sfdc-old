                    } else if({!lmsConfig.Show_Score}) {
                        if (statusObject[arr2[0]].score != arr2[3]) {
                            if (!((statusObject[arr2[0]].score == '' || statusObject[arr2[0]].score == ' ') && (arr2[3] == '' || arr2[3] == ''))) {
                                if(arr2[3] != 'null') {
                                    jQuery('td.m-col5[name="'+arr2[0]+'"]').text(arr2[3]);
                                }
