                                        <img title="" class="m-tips" alt="{!$Label.Click_here_for_details}" src="{!URLFOR($Resource.lmsilt__devplan, 'i/info-ico.png')}" align="right" />
                                    </apex:outputLink>
                                </apex:outputPanel>
                                </div>
                            </apex:column>
                            <apex:column styleClass="m-col-descr" rendered="{!lmsConfig.Show_Description}">
                                <p><pre><apex:outputText value="{!idLO_DescLO[M.MItem.lmscons__Training_Content__c]}"  /></pre></p>
