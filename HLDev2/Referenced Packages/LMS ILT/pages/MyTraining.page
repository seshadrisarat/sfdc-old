                <apex:column styleClass="m-col7-1" rendered="{!IF(lmsConfig.Show_Completed_Date, IF(ISNULL(c.TitleCurriculum), false, IF(isConsumerUser, true, false)), false)}">
                <apex:facet name="header" >
                    COMPLETED DATE
                </apex:facet>
            				<apex:outputText value="{!c.completedDateFormatted}"/>
                </apex:column>
                <!-- adde new field completed date -->
                <apex:column styleClass="m-col8" rendered="{!IF(ISNULL(c.TitleCurriculum), false, IF(isConsumerUser, true, false))}">

                </apex:column>
                <apex:column styleClass="m-col9" rendered="{!IF(lmsConfig.Enable_certificate,IF(ISNULL(c.TitleCurriculum), false, IF(isConsumerUser, true, false)), false)}">
