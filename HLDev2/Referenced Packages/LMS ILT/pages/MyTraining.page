                        <apex:outputLink value="javascript:void(0);" onclick="javascript:showInfo('{!$ObjectType.lmscons__Learning_Path__c.Name}','{!LP.LPItem.lmscons__Learning_Path_Assignment__r.lmscons__Learning_Path__c}', event,'learningPath','{!LP.LPItem.lmscons__Learning_Path_Assignment__c}')">
