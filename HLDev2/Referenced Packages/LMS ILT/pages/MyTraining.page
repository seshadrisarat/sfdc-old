                <apex:column headerClass="m-col7-1" rendered="{!IF(lmsConfig.Show_Completed_Date, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col7-1">
                <apex:facet name="header" >
                    COMPLETED DATE
                </apex:facet>
                </apex:column>
                <!-- adde new field completed date -->
                <apex:column headerClass="m-col8" rendered="{!IF(ISNULL(c.TitleCurriculum), true, false)}" styleClass="emptyFix m-col8">
                &nbsp;
