                <apex:column styleClass="m-hide-tbl m-first-level" colspan="{!Columns}" breakBefore="true" rendered="{!isConsumerUser}" >
                <apex:dataTable width="100%" value="{!c.LpEntryList}" var="LP" border="0" styleClass="{!IF(ISNULL(c.TitleCurriculum), '', 'm-hide')}" rendered="{!c.CuSeqList.size == 0}">
                    <apex:column styleClass="m-plus m-col1" style="{!IF(ISNULL(c.TitleCurriculum), 'background-position: 9px 17px;', '')}">
