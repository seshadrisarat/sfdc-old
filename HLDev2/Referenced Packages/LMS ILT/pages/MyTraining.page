        for (i in data.Sessions) {

            if(data.Sessions[i].SessionId == 0) {
                strType = '{!$ObjectType.lmsilt__Event__c.Name}';
                Str = 'Javascript:showEdit(\'' + strType + '\',\'' + data.Sessions[i].EventId + '\')';
                jQuery('.Meeting_Update_'+data.Sessions[i].ClassId).find('A').each(function(index) {
                jQuery(this).text('{!$Label.Details}');
                jQuery(this).attr('target', '');
                jQuery(this).attr('href', Str);
                });
            } else {
                Str = '{!$Page.lmsilt__JoinMeeting}?id='+data.Sessions[i].SessionId;
