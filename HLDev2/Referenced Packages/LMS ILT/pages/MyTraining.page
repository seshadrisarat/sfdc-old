    <h1 class="b-title"><img src="{!URLFOR($Resource.devplan, 'i/header-icon.png')}" />{!$Label.My_Training} </h1>
    <div class="b-btns">
        <div class="m-calendar">
            <!--  <a class="b-btn" href="{!$Page.lmsilt__Calendar}"><span class="b-icon">&nbsp;</span>{!$Label.MyTraining_Training_Calendar}</a> -->
            <apex:outputlink value="{!$Page.lmsilt__Calendar}" styleClass="b-btn" rendered="{!ILTConfig.lmsilt__Enable_My_Training_Calendar__c}"><span class="b-icon">&nbsp;</span>{!$Label.MyTraining_Training_Calendar}</apex:outputlink>
        </div>
        <apex:outputPanel rendered="{!lmsConfig.Enable_Catalog_Search}" layout="block" styleclass="m-search">
        <a href="Javascript:open_sc_popup();" class="b-btn" ><span class="b-icon">&nbsp;</span>{!$Label.Search_the_Catalog}</a>
        </apex:outputPanel>
    </div>
</div>
</apex:outputPanel>
    <apex:pageMessages id="messages" />
<apex:outputPanel rendered="{!IF((lmsConfig.Social_Learning && showHeader && isConsumerUser && showHeader), true, false)}" layout="block" styleclass="m-search">
    <iframe id="social_learning" name="social_learning" style="z-index: 1;" frameborder="0" height="0px" src="{!$Page.lmscons__AddConsumerUsersBatchable}?url={!Domain}{!URLFOR($Resource.resize, 'hw.html')}" width="100%"></iframe>
</apex:outputPanel>
<div class="l-content">

    <div style="padding-bottom: 10px; padding-left: 5px">
        <apex:outputPanel rendered="{!showHeader}">
        <apex:outputField value="{!settingsILT.lmscons__Custom_Message__c}" rendered="{!settingsILT != NULL}"/>
        </apex:outputPanel>
    </div>
    <div class="waitingHolderOffset" style="visibility:visible; width: 50%; position:relative;z-index:1000; left:50%; top:80px;">
        <img src="/img/loading.gif" class="waitingImage" title="{-!$Label.Please_wait}"/>
        <span class="waitingDescription">{!$Label.loading_label}</span>
    </div>
    <div class="b-tabs">
    <apex:outputPanel layout="none" rendered="{!SFDCDemo = false}">
        <div class="b-tab {!IF(SelectedTab == 1, 'm-active', '')}" id="dp_tab1"><a href="{!$Page.lmsilt__MyTraining}?tab=1{!additionalurlParam}">{!$Label.MyTraining_Learning_Tab}</a></div>
    <apex:outputPanel layout="none" rendered="{!IsDPEx == true}">
        <div class="b-tab {!IF(SelectedTab == 2, 'm-active', '')}" id="dp_tab2"><a href="{!$Page.lmsilt__MyTraining}?tab=2{!additionalurlParam}">{!$Label.MyTraining_Development_Plans_Tab}</a></div>
    </apex:outputPanel>
    </apex:outputPanel>
    <apex:outputPanel layout="none" rendered="{!SFDCDemo = true}">
        <apex:outputPanel layout="none" rendered="{!IsDPEx == true}" >
        <div class="b-tab {!IF(SelectedTab == 2, 'm-active', '')}" id="dp_tab2"><a href="{!$Page.lmsilt__MyTraining}?tab=2{!additionalurlParam}">{!$Label.MyTraining_Development_Plans_Tab}</a></div>
        </apex:outputPanel>
        <div class="b-tab {!IF(SelectedTab == 1, 'm-active', '')}" id="dp_tab1"><a href="{!$Page.lmsilt__MyTraining}?tab=1{!additionalurlParam}">{!$Label.MyTraining_Learning_Tab}</a></div>
    </apex:outputPanel>
        <apex:outputPanel rendered="{!lmsConfig.Social_Learning && showHeader}" layout="block" styleclass="m-search">
        <div class="b-tab {!IF(SelectedTab == 3, 'm-active', '')}" id="dp_tab3"><a href="{!$Page.lmsilt__MyTraining}?tab=3{!additionalurlParam}">{!$Label.Social_Learning_Tabs}</a></div>
        </apex:outputPanel>
        <apex:outputpanel rendered="{!NOT(SelectedTab = 3)}">
        <div class="b-setting" style="z-index: 0;">
            <div class="b-checkbox" style="z-index: 0;">
                <!-- <input type="checkbox" id="showCompleteCheckBox" onchange="RefreshParametersPage(this)"/><label for="showCompleteCheckBox">{!$Label.Show_Completed_Training}</label> -->
                <apex:outputLink onclick="document.cookie = 'apex__checkBox=false;expires=-1;path=/apex/;' ;return null;" value="{!$Page.lmsilt__MyTraining}?checkBox=false&tab={!SelectedTab}{!additionalurlParam}" rendered="{!completedItems}">{!$Label.Active_Training}</apex:outputLink>
                <apex:outputLink onclick="document.cookie = 'apex__checkBox=true;expires=-1;path=/apex/;';return null" value="{!$Page.lmsilt__MyTraining}?checkBox=true&tab={!SelectedTab}{!additionalurlParam}" rendered="{!NOT(completedItems)}">{!$Label.Completed_Training}</apex:outputLink>
            </div>
        </div>
        </apex:outputpanel>
    </div>
    <apex:actionStatus id="loadStatus">
        <apex:facet name="start">
            <c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="1" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.Loading}" messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
        </apex:facet>
    </apex:actionStatus>
    <apex:outputpanel layout="block" styleclass="b-tab-bodies" id="all_tabs">
        <apex:outputpanel layout="block" styleclass="b-tab-body" id="mytraining_block" rendered="{!SelectedTab = 1}">
        <!-- Start mytraining -->

            <apex:outputPanel rendered="{!TrainingList2.size==0}">
                <table width="100%" border="0" class="m-tbl">
                <tr>
                    <td align="center">{!$Label.You_do_not_have_any_current_training_to_complete}</td>
                </tr>
                </table>
            </apex:outputPanel>

            <apex:dataTable rendered="{!TrainingList2.size!=0}" headerClass="t-head-top" value="{!TrainingList2}" var="c" width="100%" border="0" styleClass="m-tbl">
                <!-- ------- -->
                <apex:column headerClass="m-col1" styleClass="emptyFix m-col1 {!IF(ISNULL(c.TitleCurriculum), '', 'm-plus')}" rendered="{!IF(ISNULL(c.TitleCurriculum), true, false)}">
                <apex:facet name="header" >
                    {!$Label.TRAINING}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col-descr" styleClass="emptyFix m-col-descr {!IF(ISNULL(c.TitleCurriculum), '', '')}" rendered="{!IF(lmsConfig.Show_Description, IF(ISNULL(c.TitleCurriculum), true, false), false)}">
                <apex:facet name="header" >
                    {!$Label.MyTraining_DESCRIPTION}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col2" rendered="{!IF(lmsConfig.Show_Type, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col2">
                <apex:facet name="header" >
                    {!$Label.TRAINING_TYPE}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col3" rendered="{!IF(lmsConfig.Show_status, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col3">
                <apex:facet name="header" >
                    {!$Label.STATUS}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col4" rendered="{!IF(lmsConfig.Show_progress_bar, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col4">
                <apex:facet name="header" >
                    {!$Label.PROGRESS}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col5" rendered="{!IF(lmsConfig.Show_Score, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col5">
                <apex:facet name="header" >
                    {!$Label.MyTraining_SCORE}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col6" rendered="{!IF(lmsConfig.Show_Duration, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col6">
                <apex:facet name="header" >
                    {!$Label.DURATION}
                </apex:facet>
                </apex:column>
                <apex:column headerClass="m-col7" rendered="{!IF(lmsConfig.Show_Due_Date, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col7">
                <apex:facet name="header" >
                    {!$Label.MyTraining_DUE_DATE}
                </apex:facet>
                </apex:column>
