                                <apex:outputPanel rendered="{!M.MItem.lmscons__Training_Content__r.lmscons__Quiz__c != null}">
                                <apex:outputLink rendered="{!SFDCDemo}" target="_blank" value="{!BaseUrl}?contentid={!M.MItem.lmscons__Training_Content__r.lmscons__Quiz__c}">
