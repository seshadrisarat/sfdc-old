                                </apex:outputLink>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!M.MItem.lmscons__Training_Content__r.lmsilt__Event__c != null}">
                                <apex:outputLink rendered="{!SFDCDemo}" target="_blank" value="{!BaseUrl}?contentid={!M.MItem.lmscons__Training_Content__r.lmsilt__Event__c}">
