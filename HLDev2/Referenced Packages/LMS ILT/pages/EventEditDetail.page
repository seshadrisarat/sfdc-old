				<apex:inputField value="{!lmsilt__Event__c.lmsilt__Product_Category__c}"/>

				<apex:inputField value="{!lmsilt__Event__c.Name}" required="true"/>
				<apex:inputField value="{!lmsilt__Event__c.lmsilt__Duration__c}"/>
