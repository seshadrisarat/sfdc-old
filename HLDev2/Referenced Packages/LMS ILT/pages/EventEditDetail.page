					</apex:outputPanel>
				</apex:pageBlockSectionItem>
			</apex:pageBlockSection>

			<apex:pageBlockSection columns="1" id="SessionSection" title="{!$ObjectType.lmsilt__Session__c.Label}">

				<!-- New timezones picklist -->
