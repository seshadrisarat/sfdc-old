        <label>Activity Subject</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Automatic_Logging__c</fullName>
        <defaultValue>false</defaultValue>
