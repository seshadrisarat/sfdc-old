        <label>Envelope Template Id</label>
        <length>15</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Follow_up_Interval__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
