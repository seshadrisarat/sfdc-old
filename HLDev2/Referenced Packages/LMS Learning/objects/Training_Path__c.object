    <validationRules>
        <fullName>Not_Valid_Duration_Value</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(DurationTime__c , &quot;(((((\\d+?[d]\\b)\\s?){0,1})|\\d{0,4})?((((\\d{1,2}?[h]\\b)\\s?){0,1})|\\d{0,4})?((((\\d{1,2}?[m]\\b)\\s?){0,1})|\\d{0,4})?)&quot;))</errorConditionFormula>
        <errorDisplayField>DurationTime__c</errorDisplayField>
        <errorMessage>NOT VALID DURATION</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Valid_Minutes_For_Completion_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Minutes_For_Completion__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Minutes_For_Completion__c</errorDisplayField>
        <errorMessage>this field cannot have a negative value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_valid_number</fullName>
        <active>true</active>
        <errorConditionFormula>Minutes_For_Completion__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Minutes_For_Completion__c</errorDisplayField>
        <errorMessage>Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Duration</fullName>
        <active>true</active>
        <description>Do not enter a negative value to the &apos;Duration&apos; filed.</description>
        <errorConditionFormula>Duration__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Duration__c</errorDisplayField>
        <errorMessage>this field cannot have a negative value</errorMessage>
    </validationRules>
