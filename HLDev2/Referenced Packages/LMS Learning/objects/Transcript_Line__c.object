    <validationRules>
        <fullName>validate_Due_Date</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW()  &amp;&amp;  ( Due_Date__c &lt;  TODAY() )</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Inserting of due date should not be less than the current one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validate_percent_complete</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
Percent_Complete__c &lt; 0,
Percent_Complete__c &gt; 1
)
/*ISCHANGED(Percent_Complete__c)*/</errorConditionFormula>
        <errorDisplayField>Percent_Complete__c</errorDisplayField>
        <errorMessage>Percent complete must be between 0 and 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validate_score</fullName>
        <active>true</active>
        <description>Check that in the Score have positive number</description>
        <errorConditionFormula>IF( ISBLANK(Score__c),null,
(OR (NOT(ISNUMBER(Score__c)), VALUE(Score__c) &lt; 0))
)</errorConditionFormula>
        <errorDisplayField>Score__c</errorDisplayField>
        <errorMessage>Score must be a positive number.</errorMessage>
    </validationRules>
