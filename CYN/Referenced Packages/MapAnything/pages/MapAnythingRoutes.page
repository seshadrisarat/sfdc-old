                var type = $('[id$=":dispostionSubType"]').val();
                $info.text('This will result in a 6 month period before the next visit. Are you sure you want to select ' + type + '?');
                $('.alert-body').append($info);
            </script>
        </apex:outputPanel>
        
        <!-- Disposition Next Visit View -->
