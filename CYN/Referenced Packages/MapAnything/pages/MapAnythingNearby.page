                        var refreshTimeoutValue = parseInt(refreshIntervalParts[0]) * (refreshUnit == 'sec' ? 1000 : 60000);
                        setTimeout(function () { refreshQuery($plottedQuery, refreshTimeoutValue); }, refreshTimeoutValue);
