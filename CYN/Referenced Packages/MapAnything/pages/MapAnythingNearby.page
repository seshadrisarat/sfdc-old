        var timeFormat = '{!timeFormat}';
        var dateTimeFormat = '{!dateTimeFormat}';
        var myPostionMarker;
        
        var mapLoaded = false;
        $(function () {
            
            //unbind shortcuts
            $(document).unbind('keydown.zoom');
            $(document).unbind('keyup.search');
            $(document).unbind('keyup.home');
            $(document).unbind('keyup.position');
            $(document).unbind('keyup.traffic');
            
