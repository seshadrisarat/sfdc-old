                                var hitTestGood = MA.Map.hitTestShapeMgr.containsLatLng(record.marker.getPosition());

                                //add it to the list if needed
                                if ($plottedQuery.find('.renderButtons-button.markers').is('.on') && (hitTestGood || queryProximityEnabled)) {
