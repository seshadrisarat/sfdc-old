        }
        
    });
    
    /* This is causing some issues and it's going to run too soon anyway (before query rendering is complete) so I'm removing it for now
    visibilityUpdating = false;
    if (visibilityQueued) {
        ChangeVisibilityWhenCircleIsAdded();
    }
