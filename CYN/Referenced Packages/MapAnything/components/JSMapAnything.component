                            contactIds.push(record.record.Id);
                        }
                        else if ((record.isVisible || record.isClustered || record.isScattered) && record.record.Id.substring(0, 3) == '00Q')   //lead
                        {
                            leadIds.push(record.record.Id);
                        }
