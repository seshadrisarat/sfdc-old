                action: 'email_directions',
                replyTo: UserEmailAddress,
                subject: 'Map Anything Directions',
                toAddresses: EmailAddresses.split(";"),
                htmlBody: htmlBody              
        };
        
        $.ajax({
            url: MA.resources.Email,
            type: 'POST',
            dataType: 'JSON',
            data: { MyJSObject: JSON.stringify(MyJSObject) },
            complete: function (response)
