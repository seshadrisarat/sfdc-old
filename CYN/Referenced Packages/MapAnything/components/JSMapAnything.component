                }
            }
        });
        
    }
    else
    {
        showError($('#EditPermissionsPopup .loadmask-wrapper'), 'Please select a User/Profile');
    }
    
    $('#permissions-grid-wrapper').removeClass('permissions-grid-loading');
}

