							}

							colVisible = true;

						} else {
							colContents = '';
							// colVisible = false;
						}
					}
				}
				else
				{
					colVisible = false;
				}

				if (options.isSelectedTab && options.toolTipIndex < MAListView.listViewSettings.selectedTooltips)
				{
					colVisible = true;
				}
				
				return {
					colContents : colContents,
