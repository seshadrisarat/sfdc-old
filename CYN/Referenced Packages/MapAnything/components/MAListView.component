					var options = {
						layerId			: 'string',
						recordsList		: [],
						listViewSettings: {
											pageSize: 10,
											pageNumber: 1,
											startIndex: 0,
											selectedIds: [],
											currentSort: { columnToSort: '', sortAscending: true },
