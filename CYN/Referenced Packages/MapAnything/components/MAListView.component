				}
				else
				{

					if (options.layerData.tooltips[options.tooltipIndex])
					{
					
					
						if ($.inArray( options.layerData.tooltips[options.tooltipIndex].DisplayType.toUpperCase(), [ 'STRING','PHONE','ID','Email','Combobox','MULTIPICKLIST','PHONE','TEXTAREA','URL' ] ) > -1)
						{
