            // Updates the page number and records in the pagination area
			UpdatePageShowing: function(options) {
				var countHTML = '';

				if(options.layerId === 'SelectedTab') {
					var filterCount = MAListView.listViewSettings.filterCount;
					var total = filterCount > 0 ? filterCount : MAListView.recordsList.length;

					var start = MAListView.listViewSettings.startIndex + 1;
