            // Processes the tooltip columns
			ProcessToolTipColumn: function(options) {
			
				/*
					Input: {
							toolTipIndex : int,
							record : {},
							layerData : {},
							isSelectedTab: boolean
						}
					
					Output: {
						colContents: 'string',
